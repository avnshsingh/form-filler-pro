{
  "manifest_version": 3,
  "name": "Form Filler Pro",
  "version": "1.0.1",
  "action": { "default_popup": "index.html" },
  "icons": {
    "128": "src/assets/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage", "activeTab"],
  "options_page": "src/options.html"
}
